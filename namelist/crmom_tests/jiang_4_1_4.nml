This namelist contains various input parameters for RAMSES runs

&RUN_PARAMS
hydro=.true.
nremap=0
ncontrol=100
nsubcycle=4*1,
static_gas=.false.
/

&AMR_PARAMS 
levelmin=6
levelmax=6
ngridmax=10000
nexpand=1
boxlen=1.0
/

&BOUNDARY_PARAMS
nboundary=2
ibound_min=-1,+1
ibound_max=-1,+1
bound_type= 0, 0
/

&MHD_PARAMS
Dcr=0.1
cr_c_fraction=3.3356409297285193e-11  ! light-speed=1
!cr_c_fraction=3.3356409297285193e-9  ! light-speed=1
/

&INIT_PARAMS
nregion=1
region_type(1)='square'
x_center=0.5,1.5
length_x=1.0,1.0
d_region=1.0,0.2
u_region=0.0,1.186
v_region=0.0,2.967
w_region=0.0,0.0
p_region=2.0,0.1368
A_region=1.0,1.0
B_region=0.0,1.6405
C_region=0.0,0.0
/

&OUTPUT_PARAMS
foutput=1
noutput=1
tout=0.5
/

&HYDRO_PARAMS
gamma=1.4
courant_factor=0.8
slope_type=1
riemann='hlld'
/

&REFINE_PARAMS 
err_grad_d=0.01
err_grad_u=0.05
err_grad_p=0.05
interpol_var=0
interpol_type=1
/



